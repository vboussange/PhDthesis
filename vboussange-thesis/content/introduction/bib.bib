keywords = {Biological analogy,Bounded rationality,Complex system,Constrained optimization,Dissipative structure,Economic emergence,Economic growth,Emotions,Energy economics,Entrepreneurship,Evolutionary economics,Innovation,Institutional change,Knowledge,Micro-meso-macro,Organizational change,Rationality,Self organization,Technological change,Uncertainty},
mendeley-groups = {Complexity/Project_Proposal/analogy-economics-biology/Thermodynamics},
