publisher = {Bloomsbury Academic},
title = {{Towards an Evolutionary Theory of Economic Change}},
url = {http://www.bloomsburycollections.com/book/long-run-economics-an-evolutionary-approach-to-economic-growth/ch5-towards-an-evolutionary-theory-of-economic-change},
year = {2014}
}
