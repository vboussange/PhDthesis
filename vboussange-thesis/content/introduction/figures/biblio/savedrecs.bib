@article{ WOS:000398685600006,
Author = {Wickman, Jonas and Diehl, Sebastian and Blasius, Bernd and Klausmeier,
   Christopher A. and Ryabov, Alexey B. and Brannstrom, Ake},
Title = {Determining Selection across Heterogeneous Landscapes: A
   Perturbation-Based Method and Its Application to Modeling Evolution in
   Space},
