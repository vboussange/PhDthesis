@article{ WOS:000585759400001,
Author = {Lepers, Clotilde and Billiard, Sylvain and Porte, Matthieu and Meleard,
   Sylvie and Viet Chi Tran},
Title = {Inference with selection, varying population size, and evolving
   population structure: application of ABC to a forward-backward
   coalescent process with interactions},
