journal = {bioRxiv},
keywords = {allomet-34,body size,microhabitat use,non-consumptive predator-predator effects},
mendeley-groups = {Complexity/Project_Proposal/econobio/parameter_estimation_in_ecosystem},
pages = {2021.05.06.442910},
title = {{From theory to experiment and back again — Challenges in quantifying a trait-based theory of predator-prey dynamics}},
