month = {nov},
pages = {817--825},
publisher = {Elsevier Inc.},
title = {{Lotka–Volterra model parameter estimation using experiential data}},
url = {http://dx.doi.org/10.1016/j.amc.2013.08.093 https://linkinghub.elsevier.com/retrieve/pii/S0096300313009806},
