month = {may},
number = {May},
pages = {1--22},
title = {{Allometric Trophic Networks From Individuals to Socio-Ecosystems: Consumer–Resource Theory of the Ecological Elephant in the Room}},
url = {https://www.frontiersin.org/article/10.3389/fevo.2020.00092/full},
volume = {8},
year = {2020}
}
