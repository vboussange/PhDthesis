file = {:Users/victorboussange/Library/Application Support/Mendeley Desktop/Downloaded/Ding et al. - 2020 - Ancient orogenic and monsoon-driven assembly of the world's richest temperate alpine flora(2).pdf:pdf},
issn = {0036-8075},
journal = {Science},
