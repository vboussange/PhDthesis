keywords = {Chaos,Food chain,Food web,Functional response,Predation,Prey preference},
mendeley-groups = {Complexity/Project_Proposal/Parameter estimation/models},
number = {1},
pages = {8--14},
title = {{Prey preference by a top predator and the stability of linked food chains}},
volume = {81},
year = {2000}
}
