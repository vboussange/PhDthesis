doi = {10.1109/HPEC49654.2021.9622796},
eprint = {1812.01892},
file = {:Users/victorboussange/Downloads/1812.01892.pdf:pdf},
isbn = {978-1-6654-2369-4},
mendeley-groups = {Complexity/Project_Proposal/Parameter estimation/sensisitivty analysis},
month = {sep},
number = {2},
pages = {1--9},
publisher = {IEEE},
title = {{A Comparison of Automatic Differentiation and Continuous Sensitivity Analysis for Derivatives of Differential Equation Solutions}},
