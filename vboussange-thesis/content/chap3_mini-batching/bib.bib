issn = {1364-5021},
journal = {Proceedings of the Royal Society A: Mathematical, Physical and Engineering Sciences},
keywords = {Data-driven discovery,Information criteria,Model selection,Nonlinear dynamics,Sparse regression},
mendeley-groups = {Complexity/Project_Proposal/econobio/model selection,Complexity/Project_Proposal/sci-ml,Complexity/Project_Proposal/Parameter estimation/discovery},
month = {aug},
number = {2204},
